{
  "name": "febp",
  "version": "0.0.0-PLACEHOLDER",
  "description": "Frontend boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/iboozyvoozy/febp.git"
  },
  "scripts": {
    "postinstall": "typings install && webdriver-manager update",
    "start": "npm run build",
    "test": "npm-run-all test:unit coverage:*",
    "test:e2e": "protractor build/protractor.prod.js",
    "test:e2e:dev": "protractor build/protractor.dev.js",
    "validate": "npm-run-all --parallel validate:* lint",
    "validate:dev": "webpack-validator build/webpack.dev.js",
    "validate:test": "webpack-validator build/webpack.test.js",
    "validate:prod": "webpack-validator build/webpack.prod.js",
    "lint": "eslint config",
    "serve": "node server.js",
    "serve:dev": "browser-sync start --config build/browser-sync.js",
    "serve:mocks": "json-server mocks/api.js --config mocks/config.json",
    "serve:webdriver": "webdriver-manager start",
    "build": "webpack --config build/webpack.prod.js",
    "build:dev": "webpack --config build/webpack.dev.js",
    "test:unit": "karma start build/karma.prod.js",
    "test:unit:dev": "karma start build/karma.dev.js",
    "coverage:check": "istanbul check-coverage",
    "coverage:report": "cat ./reports/coverage/lcov.info | codeclimate-test-reporter",
    "commit": "git-cz",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "dependencies": {
    "angular": "1.5.9",
    "angular-mocks": "1.5.8",
    "autoprefixer": "6.5.3",
    "awesome-typescript-loader": "1",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "clean-webpack-plugin": "0.1.14",
    "codeclimate-test-reporter": "0.4.0",
    "commitizen": "2.8.6",
    "compression": "1.6.2",
    "css-loader": "0.25.0",
    "cz-conventional-changelog": "1.2.0",
    "debug": "2.5.2",
    "dotenv": "2.0.0",
    "eslint": "3.10.2",
    "eslint-loader": "1.6.1",
    "exports-loader": "0.6.3",
    "express": "4.14.0",
    "express-history-api-fallback": "2.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "imports-loader": "0.6.5",
    "jade": "1.11.0",
    "jade-html-loader": "0.0.3",
    "json-loader": "0.5.4",
    "lodash": "4.17.3",
    "ng-annotate-loader": "0.2.0",
    "ng-cache-loader": "0.0.21",
    "npm-run-all": "3.1.2",
    "postcss-loader": "1.1.0",
    "pug": "2.0.0-beta4",
    "pug-loader": "2.3.0",
    "require-environment-variables": "1.1.2",
    "semantic-release": "4.3.5",
    "style-loader": "0.16.1",
    "stylint": "1.5.7",
    "stylint-loader": "1.0.0",
    "stylus": "0.54.5",
    "stylus-loader": "2.3.1",
    "tslint": "3.15.1",
    "tslint-loader": "2.1.5",
    "typescript": "1.8.10",
    "typings": "1.5.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-merge": "1.0.2",
    "webpack-validator": "2.2.11"
  },
  "devDependencies": {
    "babel-plugin-__coverage__": "11.0.0",
    "babel-register": "6.18.0",
    "browser-sync": "2.18.5",
    "coveralls": "2.11.14",
    "istanbul": "0.4.5",
    "jasmine": "2.5.2",
    "json-server": "0.9.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-html-reporter": "0.2.7",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-webpack": "1.8.0",
    "protractor": "4.0.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "keywords": [
    "frontend",
    "angular",
    "webpack",
    "babel",
    "karma",
    "protractor",
    "typescript"
  ]
}
